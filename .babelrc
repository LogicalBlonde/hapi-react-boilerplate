{
  "presets": [
    ["es2015", { "modules": false}],
    "stage-0",
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    ["import", {
      "libraryName":"antd",
      "libraryDirectory":"lib",
      "style":"css"
    }]
  ],
  "env": {
    "modules":false,
    "targets":{
      "browsers": ["last 2 Chrome versions"]
    },
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
