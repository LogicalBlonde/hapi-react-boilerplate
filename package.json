{
  "name": "my-cool-app",
  "main": "index.js",
  "version": "1.0.0",
  "engines": {
    "node": "6.10.2"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "npm run server",
    "storybook": "start-storybook -p 9001",
    "test": "NODE_ENV=test jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch",
    "test:watch:coverage": "npm run test:coverage -- --watch",
    "test:snapshots:update": "npm run test -- --updateSnapshot",
    "server": "nodemon index.js",
    "client": "NODE_PATH=./src/ npm start --prefix client",
    "webpack:prod": "rm -rf ./client/build | NODE_ENV=production webpack --env=prod",
    "build:prod": "npm run webpack:prod -- --progress --profile --colors",
    "postinstall": "npm run build:prod"
  },
  "dependencies": {
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "boom": "^5.1.0",
    "concurrently": "^3.5.0",
    "dotenv": "^4.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "glue": "^4.1.0",
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.4.3",
    "hapi-swagger": "^7.7.0",
    "hapi-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.29.0",
    "inert": "^4.2.0",
    "joi": "^10.6.0",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "redux": "^3.7.1",
    "redux-actions": "^2.2.1",
    "redux-thunk": "^2.2.0",
    "vision": "^4.1.1",
    "webpack": "^3.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "./src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
